    "linq",
    "lazy"
  ],
  "author": "Sergey Shandar",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sergey-shandar/iterable-ts/issues"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "homepage": "https://github.com/sergey-shandar/iterable-ts#readme",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.37",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "typedoc": "0.5.5",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "tslib": "^1.5.0"
